algoritmo "semnome"

var
   ano, nasc, dinheiro: inteiro
inicio
      Escreva("Em que ano estamos? ")
      Leia(ano)
      Escreva("Em que ano vc nasceu? ")
      Leia(nasc)
      dinheiro <- ano - nasc
      Escreval("em ", ano, "Vc tera", idade)
      Se (dinheiro>=10000) entao
      Escreval("Voce ja tera atingido a maior idade")
      FimSe

fimalgoritmo
